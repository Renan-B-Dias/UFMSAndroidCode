<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.renanbenattidias.dbex.EmployeeListActivity">

    <ListView
        android:id="@+id/listEmployeeListView"
        android:layout_width="368dp"
        android:layout_height="399dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="@+id/listEmployeeTitle"
        app:layout_constraintStart_toStartOf="@+id/listEmployeeTitle"
        app:layout_constraintTop_toBottomOf="@+id/listEmployeeTitle" />

    <TextView
        android:id="@+id/listEmployeeTitle"
        android:layout_width="356dp"
        android:layout_height="82dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
